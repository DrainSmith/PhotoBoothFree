<Window x:Class="PhotoboothFree.PrintWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhotoboothFree"
        mc:Ignorable="d"
        Title="Tap To Close" Height="300" Width="300" Loaded="Window_Loaded" WindowStyle="None" ResizeMode="NoResize">
    <Grid MouseDown="Grid_MouseDown">
        <Image Name="ImageDisplay" Stretch="Uniform" IsHitTestVisible="False"></Image>
    </Grid>
</Window>
